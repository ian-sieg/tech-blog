<h1>Homepage</h1>

{{#each posts as |post| }}

<div class="card">
    <div class="card-header">
        <a class="card-header-title" href="/trip/{{post.id}}">{{post.title}}</a>
        <p class="ml-5 all-trips-text">
            {{post.content}}
            {{post.user.username}}
        </p>
    </div>
</div>
{{/each}}